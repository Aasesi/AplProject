<Window x:Class="Aplproject.MainWindow"
        
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="loadImageButton" Content="Load Image" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="10" Click="loadImageButton_Click"/>
        <Image x:Name="displayImage" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="Uniform"/>
    </Grid>
</Window>
